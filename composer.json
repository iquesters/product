{
  "name": "iquesters/product",
  "description": "Product management package for Laravel",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "laravel/tinker": "^2.8"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Iquesters\\Product\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iquesters\\Product\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Pritam Bag",
      "email": "pritam02b@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
        "providers": [
            "Iquesters\\Product\\ProductServiceProvider"
        ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}